<?xml version="1.0"?>
<extension key="aws-ses" type="module">
  <file>aws_ses</file>
  <name>AWS SES mail processor</name>
  <description>Processes AWS SES bounce and complaint notifications in CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Michael McAndrew</author>
    <email>michaelmcandrew@thirdsectordesign.org</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.3sd.io/extensions/aws-ses</url>
    <url desc="Documentation">https://lab.3sd.io/extensions/aws-ses</url>
    <url desc="Support">https://3sd.io</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-10-26</releaseDate>
  <version>1.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/AwsSes</namespace>
    <angularModule>crmAwsSes</angularModule>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
  </mixins>
  <upgrader>CRM_AwsSes_Upgrader</upgrader>
</extension>
