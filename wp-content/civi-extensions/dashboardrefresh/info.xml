<?xml version="1.0"?>
<extension key="dashboardrefresh" type="module">
  <file>dashboardrefresh</file>
  <name>Dashboard Refresh</name>
  <description>Adds a 'Refresh all dashlets' button to the dashboard.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Aidan Saunders</author>
    <email>aidan.saunders@squiffle.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/dashboardrefresh</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/dashboardrefresh</url>
    <url desc="Support">https://lab.civicrm.org/extensions/dashboardrefresh</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-06-26</releaseDate>
  <version>1.3</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.74</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Dashboardrefresh</namespace>
    <format>23.02.1</format>
  </civix>
</extension>
